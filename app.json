{
  "expo": {
    "name": "USAHS Reentry",
    "slug": "USAHSReentry",
    "version": "2.1.20210413",
    "orientation": "landscape",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#00677f"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "requireFullScreen": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan barcodes for campus entry verification."
      },
      "bundleIdentifier": "usahsReentryScanner"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#00677f"
      },
      "package": "com.usahs.reentryscanner"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
